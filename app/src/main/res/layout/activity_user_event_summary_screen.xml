<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.maryhuerta.cateringapp.user_event_summary_screen">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="USER EVENT SUMMARY"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="31dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="From"
        android:inputType="date"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="232dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="To"
        android:inputType="date"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="232dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter search boundaries:"
        android:textSize="24sp"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="163dp" />

    <Button
        android:id="@+id/searchEvent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="350dp" />

    <QuickContactBadge
        android:id="@+id/quickContactBadge2"
        android:layout_width="45dp"
        android:layout_height="45dp"
        app:srcCompat="@android:drawable/ic_menu_day"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="232dp" />

    <QuickContactBadge
        android:id="@+id/quickContactBadge3"
        android:layout_width="45dp"
        android:layout_height="45dp"
        app:srcCompat="@android:drawable/ic_menu_month"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="232dp" />

</android.support.constraint.ConstraintLayout>
